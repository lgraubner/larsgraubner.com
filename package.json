{
  "name": "11ty-blog",
  "version": "1.0.0",
  "description": "",
  "main": "index.js",
  "scripts": {
    "build": "npm run build:css && eleventy",
    "build:css": "postcss ./src/css/app.css -o ./_site/app.css",
    "dev": "NODE_ENV=development eleventy --serve",
    "dev:css": "postcss ./src/css/app.css -o ./_site/app.css --watch",
    "dev:all": "concurrently \"npm run dev:css\" \"npm run dev\"",
    "start": "npm run build && http-server _site -o -a localhost --silent",
    "format": "prettier --write \"./**/*.{js,md}\"",
    "test": "echo \"Error: no test specified\" && exit 1"
  },
  "keywords": [],
  "author": "Lars Graubner <mail@larsgraubner.de> (https://larsgraubner.com/)",
  "license": "MIT",
  "devDependencies": {
    "@11ty/eleventy": "1.0.0",
    "@11ty/eleventy-plugin-rss": "1.1.2",
    "@11ty/eleventy-plugin-syntaxhighlight": "3.1.3",
    "autoprefixer": "10.4.2",
    "concurrently": "7.0.0",
    "cssnano": "5.0.15",
    "date-fns": "2.28.0",
    "html-minifier": "4.0.0",
    "http-server": "14.1.0",
    "husky": "7.0.4",
    "lint-staged": "12.1.5",
    "postcss": "8.4.5",
    "postcss-cli": "9.1.0",
    "postcss-import": "14.0.2",
    "prettier": "2.5.1",
    "tailwindcss": "3.0.12"
  }
}
